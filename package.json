    "semver": "^5.4.1",
    "shortid": "^2.2.8",
    "three": "^0.83.0",
    "three-js-csg": "^72.0.0"
  },
  "devDependencies": {
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
